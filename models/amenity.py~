#!/usr/bin/python3
""" class amenity """
from models.base_model import BaseModel


class Amenity(BaseModel):
    """ Amenity class """
    def __init__(self, email = "", password = "", first_name = "", last_name = ""):
        """ Init """
        self.email = email
        self.password = password
        self.first_name = first_name
        self.last_name = last_name
