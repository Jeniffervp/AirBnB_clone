#!usr/bin/python3
""" class file_storage """
import json

class FileStorage:
    """ Clase file Storage"""

    __file_path = ""
    __objects = {}

    def __init__(self):
        """ Init """
        pass

    def all(self):
        return FileStorage.__objects

    def new(self, obj):
        FileStorage.__objects["{}.{}".format(obj.__class__.__name__, obj.id)] = obj
        print("Dictionary es: {}".format(FileStorage.__objects))

    def save(self):
        for key, value in FileStorage.__objects.items():
            print("En Save: Este es key : {} y este es el value: {}".format( key, value))
        json_string = json.dumps(obj_string)
        print("JSON string es {}:".format(json_string))
        with open(FileStorage.__file_path, 'w', encoding = "UTF8") as my_file:
            my_file.write(json_string)

    def reload(self):
        if FileStorage.__file_path:
            with open(FileStorage.__file_path, 'r', encoding = "UTF8") as my_file:
                my_file.read(json_string)
                FileStorage.__objects = json.loads(json_string)
